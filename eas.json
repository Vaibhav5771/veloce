{
  "cli": {
    "appVersionSource": "remote"
  },
  "build": {
    "debug": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      }
    },
    "profile": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "env": {
        "EXPO_PUBLIC_OPTIMIZE": "true"
      }
    },
    "release": {
      "distribution": "store",
      "android": {
        "buildType": "apk"
      }
    }
  }
}